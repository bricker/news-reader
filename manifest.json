{
  "manifest_version": 2,
  "name": "News Reader",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.cnn.com/*",
        "*://*.latimes.com/*",
        "*://*.nytimes.com/*",
        "*://*.washingtonpost.com/*"
      ],
      "js": ["filter.js"]
    }
  ],
  "browser_action": {},
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "permissions": [ "storage" ]
}
